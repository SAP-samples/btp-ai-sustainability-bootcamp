{
    "_version": "1.8.0",
    "sap.app": {
        "id": "equipmentconditions",
        "type": "application",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "dataSources": {
            "AdminService": {
                "uri": "/admin/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "sourceTemplate": {
            "id": "@sap-ux/fiori-elements-writer:lrop",
            "version": "0.2.0"
        },
        "crossNavigation": {
            "inbounds": {
                "equipmentconditions-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "EquipmentConditions",
                    "action": "Manage",
                    "title": "{{appTitle}}",
                    "subTitle": "{{appSubTitle}}",
                    "info": "Equipment Condition Monitoring by Time Period",
                    "icon": "sap-icon://machine",
                    "indicatorDataSource": {
                        "dataSource": "mainService",
                        "path": "/dynamic_dest/smart-factory-app-api/admin/EquipmentConditions/$count",
                        "refresh": 0
                    }
                }
            }
        }
    },
    "sap.platform.cf": {
        "ui5VersionNumber": "1.101.0"
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.101.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.fe.templates": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "AdminService",
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true,
                    "groupProperties": {
                        "default": {
                            "submit": "Auto"
                        }
                    }
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "sonsList",
                    "target": "EquipmentConditionsList"
                },
                {
                    "pattern": "EquipmentConditions({key}):?query:",
                    "name": "EquipmentConditionsDetails",
                    "target": "EquipmentConditionsDetails"
                }
            ],
            "targets": {
                "EquipmentConditionsList": {
                    "type": "Component",
                    "id": "EquipmentConditionsList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "EquipmentConditions",
                            "initialLoad": true,
                            "navigation": {
                                "EquipmentConditions": {
                                    "detail": {
                                        "route": "EquipmentConditionsDetails"
                                    }
                                }
                            },
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "selectionMode": "Single",
                                        "selectAll": true,
                                        "type": "ResponsiveTable"
                                    }
                                }
                            }
                        }
                    }
                },
                "EquipmentConditionsDetails": {
                    "type": "Component",
                    "id": "EquipmentConditionsDetailsList",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "EquipmentConditions",
                            "navigation": {},
                            "content": {
                                "header": {
                                    "facets": {
                                        "CustomHeaderFacetThresholdProgress": {
                                            "template": "equipmentconditions.custom.ThresholdProgressBar",
                                            "title": "No. of Anomalies Detected",
                                            "position": {
                                                "placement": "After",
                                                "anchor": "CollectionFacet1"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui": {
        "fullWidth": false,
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    },
    "sap.cloud": {
        "public": true,
        "service": "smartfactory"
    }
}